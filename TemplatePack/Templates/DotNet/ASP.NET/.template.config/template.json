{
    "$schema": "http://json.schemastore.org/template",
    "author": "Aggregate Groot",
    "classifications": [ "web", "c#" ],
    "identity": "AggregateGroot.DotNet.Asp.WebApi",
    "name": "ASP.NET Web API Project",
    "shortName": "ag-web-api",
    "sourceName": "ASP.NET_Web_API",
    "tags": {
        "language": "",
        "type": "project"
    },
    "symbols": {
        "rootNamespace": {
            "replaces": "_RootNamespace_",
            "type": "parameter",
            "datatype": "text",
            "description": "The root namespace for the project."
        },
        "dotnetVersion": {
            "replaces": "_DotNetVersion_",
            "type": "parameter",
            "datatype": "text",
            "defaultValue": "net7.0",
            "description": "The .NET version to use."
        },
        "apiPort": {
            "replaces": "_ApiPort_",
            "type": "parameter",
            "datatype": "int",
            "description": "Non secure port the API will listen on."
        },
        "apiSslPort": {
            "replaces": "_ApiSslPort_",
            "type": "parameter",
            "datatype": "int",
            "description": "Secure port the API will listen on."
        }
     }
  }